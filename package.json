{
  "name": "johnjs",
  "version": "0.1.0",
  "description": "JOHN parser and serializer for javascript",
  "main": "./src/index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nora2605/johnjs.git"
  },
  "keywords": [
    "JOHN",
    "parser",
    "serializer"
  ],
  "author": "nora2605",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nora2605/johnjs/issues"
  },
  "homepage": "https://github.com/nora2605/johnjs#readme",
  "browser": {
    "./dist/index.js": "./browser/index.js",
    "./dist/util.js": "./browser/dist/util.js",
    "./util.js": "./browser/dist/util.js"
  },
  "exports": {
    ".": {
      "node": "./dist/index.js",
      "default": "./browser/index.js"
    },
    "./package.json": "./package.json",
    "./util": {
      "node": "./dist/util.js",
      "default": "./browser/dist/util.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "scripts": {
    "test": "node ./src/tests/test.js"
  },
  "browserslist": "defaults, not ie 11",
  "prettier": {
    "arrowParens": "avoid",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^14.18.35",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.1.0",
    "fast-check": "^2.12.0",
    "jest": "^29.0.1",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "prettier": "^2.2.1",
    "rollup": "^3.7.5",
    "tslib": "^2.1.0",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">= 14",
    "npm": ">= 7"
  }
}
